{
  "name": "ngx-feature-flag",
  "version": "0.1.0-development",
  "description": "feature flag component and services for angular applications",
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "npm run remove-dist && gulp build:library",
    "build:demo": "webpack --config config/webpack.demo.js --progress --profile --bail",
    "clean": "rimraf node_modules doc coverage dist dist-demo bundles",
    "bundle-webpack": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "reinstall": "npm run clean && npm install",
    "remove-dist": "rimraf dist",
    "start": "webpack-dev-server --config config/webpack.demo.js --progress --host 0.0.0.0 --port 3001 --profile --watch --content-base src/",
    "start:demo": "webpack-dev-server --config config/webpack.demo.js --progress --host 0.0.0.0 --port 8001 --profile --watch --content-base dist-demo",
    "test": "npm run test:unit",
    "test:unit": "karma start",
    "test:debug": "karma start --no-single-run --browsers Chrome",
    "semantic-release": "semantic-release pre && npm run build && cp -r .git dist && npm publish dist"
  },
